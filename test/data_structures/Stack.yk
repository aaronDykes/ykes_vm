


class Stacky
{

    init()
    {
        this.s = Stack();
        this.size_ar = Array(["vector", "of", "vectors"]);
        this.cp_ar = Array(["This is", " a", " vector of vectors"]);
        this.vec = Vector();
    }

    res()
    {
        this.vec.push(this.size_ar);
        this.vec.push(this.cp_ar);

        this.s.push(this.vec);
        this.s.push(this.cp_ar);
        this.s.push(this.size_ar);

        this.s.pop();



        each (var i : this.s)
            pout(i);

    }

    destroy()
    {
        rm(this.s);
    }


}
