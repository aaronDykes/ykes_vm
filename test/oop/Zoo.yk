class Zoo
{
    init() 
    {
        this.aardvark = 1;
        this.baboon   = 1;
        this.cat      = 1;
        this.donkey   = 1;
        this.elephant = 1;
        this.fox      = 1;
        this.sum      = 0;
    }
    ant()    { return this.aardvark; }
    banana() { return this.baboon; }
    tuna()   { return this.cat; }
    hay()    { return this.donkey; }
    grass()  { return this.elephant; }
    mouse()  { return this.fox; }

    

    res()
    {

        while (this.sum < 10000000000) {
            this.sum = this.sum + this.ant()
                + this.banana()
                + this.tuna()
                + this.hay()
                + this.grass()
                + this.mouse();
        }
        pout(
            this.sum
        );

    }

}




