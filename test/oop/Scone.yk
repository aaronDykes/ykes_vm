class Scone
{
    

    init(first, second, third)
    {
        this.first = first;
        this.second = second;
        this.third = third;
    }
    first()
    {
        return this.first;
    }
    second()
    {
        return this.second;
    }
    third()
    {
        return this.third;
    }
    topping()
    {
        pout(this.first() + " scones with " + this.second() + "and " + this.third());
    }
}


