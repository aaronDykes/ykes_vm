
class Fizzbang
{

    fb()
    {
        var str;
        for (var i = 1; i < 100; i++)
            if (i % 15 == 0)
            {
                str = "FIZZBANG: " + i;
                pout(str);
            }
            elif (i % 5== 0)
            {
                str = "BANG: " + i;
                pout(str);
            }
            elif (i % 3 == 0)
            {
                str = "FIZZ: " + i;
                pout(str);
            }
            else 
                pout(i);

    }
}