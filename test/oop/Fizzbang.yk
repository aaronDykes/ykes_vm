
class Fizzbang
{

    init(n)
    {
        this.n = n;
    }

    res()
    {
        var str;
        for (var i = 1; i < this.n; i++)
            if (i % 15 == 0)
            {
                str = "FIZZBANG: " + i;
                pout(str);
            }
            elif (i % 5 == 0)
            {
                str = "BANG: " + i;
                pout(str);
            }
            elif (i % 3 == 0)
            {
                str = "FIZZ: " + i;
                pout(str);
            }
            else 
                pout(i);

    }
}